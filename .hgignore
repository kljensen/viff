# This is actually not a -*- shell-script -*- but the syntax is
# close enough...

# use shell glob syntax (the alternative is Python regexp syntax)
syntax: glob

*.pyc

# ignore backup files
*~
*.bak
*.orig


# ignore keys and certificates
*-[1-9].{ini,cert,key}
ca.{cert,key}

# ignore Trial output directory
_trial_temp

# ignore benchmarking data
profile.data
*.prof

# ignore LaTeX outputs
*.{aux,dvi,log,nav,out,pdf,ps,snm,toc,vrb}
doc/api/

# ignore Distutils generated files
dist
MANIFEST

# ignore trace2html.py's standard output directory
coverage_dir

# ignore eclipse project files
.project

# ignore eclipse pydev extension project files
.pydevproject

# ignore eclipse settings
.settings
